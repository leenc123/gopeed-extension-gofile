{
  "name": "gopeed-extention-gofile",
  "author": "leen",
  "title": "Gofile拓展",
  "description": "支持复制Gofile网站的下载地址下载文件",
  "icon": "icon.png",
  "version": "1.0.1",
  "homepage": "https://github.com/leenc123/gopeed-extension-gofile",
  "repository": {
    "url": "https://github.com/leenc123/gopeed-extension-gofile"
  },
  "scripts": [
    {
      "event": "onResolve",
      "match": {
        "urls": ["*://gofile.io/*"]
      },
      "entry": "dist/index.js"
    }
  ],
  "settings": [
    {
      "name": "authorization",
      "title": "Authorization",
      "description": "token认证参数",
      "type": "string"
    },
    {
      "name": "accountToken",
      "title": "AccountToken",
      "description": "cookieToken",
      "type": "string"
    }
  ]
}